{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": ">=7.4",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "ext-openssl": "*",
        "ext-pcntl": "*",
        "ext-pdo": "*",
        "ext-posix": "*",
        "ext-redis": "*",
        "composer/composer": ">=2.0",
        "cron/cron": ">=1.4",
        "damienharper/auditor-bundle": ">=4.0",
        "doctrine/doctrine-bundle": "*",
        "doctrine/orm": "*",
        "friendsofsymfony/rest-bundle": ">=3.0",
        "lexik/jwt-authentication-bundle": ">=2.11",
        "nelmio/api-doc-bundle": ">=4.0",
        "nelmio/cors-bundle": ">=2.0",
        "ramsey/uuid-doctrine": ">=1.6",
        "sensio/framework-extra-bundle": ">=5.5",
        "snc/redis-bundle": ">=3.0",
        "stof/doctrine-extensions-bundle": ">=1.4",
        "symfony/asset": ">=5.2",
        "symfony/console": ">=5.2",
        "symfony/dotenv": ">=5.2",
        "symfony/flex": ">=1.3.1",
        "symfony/form": ">=5.2",
        "symfony/framework-bundle": ">=5.2",
        "symfony/http-client": ">=5.2",
        "symfony/inflector": ">=5.2",
        "symfony/lock": ">=5.2",
        "symfony/messenger": ">=5.2",
        "symfony/mime": ">=5.2",
        "symfony/monolog-bundle": ">=3.5",
        "symfony/serializer-pack": ">=1.0",
        "symfony/string": ">=5.2",
        "symfony/twig-pack": ">=1.0",
        "symfony/validator": ">=5.2",
        "symfony/var-dumper": ">=5.2",
        "symfony/web-link": ">=5.2",
        "symfony/yaml": ">=5.2",
        "vich/uploader-bundle": ">=1.13"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": ">=3.3",
        "friendsofphp/php-cs-fixer": ">=2.16",
        "symfony/maker-bundle": ">=1.19",
        "symfony/stopwatch": ">=5.2",
        "symfony/twig-bundle": ">=5.2",
        "symfony/web-profiler-bundle": ">=5.2"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "KejawenLab\\ApiSkeleton\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "KejawenLab\\ApiSkeleton\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "KejawenLab\\ApiSkeleton\\Composer\\ScriptHandler::preInstall",
            "@auto-scripts",
            "KejawenLab\\ApiSkeleton\\Composer\\ScriptHandler::postInstall"
        ],
        "post-update-cmd": [
            "KejawenLab\\ApiSkeleton\\Composer\\ScriptHandler::preInstall",
            "@auto-scripts",
            "KejawenLab\\ApiSkeleton\\Composer\\ScriptHandler::postInstall"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": ">=5.2"
        }
    }
}
