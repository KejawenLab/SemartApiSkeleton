{% apply spaceless %}
    <script src="{{ preload(asset('plugins/jquery/jquery.min.js'), {as: 'javascript'}) }}"></script>
    <script src="{{ preload(asset('plugins/bootstrap/js/bootstrap.bundle.min.js'), {as: 'javascript'}) }}"></script>
    <script src="{{ preload(asset('dist/js/adminlte.min.js'), {as: 'javascript'}) }}"></script>
    {{ javascripts | raw }}
    <script>
        function getParameterByName(name, url) {
            if (!url) url = window.location.href;
            name = name.replace(/[\[\]]/g, '\\$&');
            let regex = new RegExp('[?&]' + name + '(=([^&#]*)|&|#|$)'), results = regex.exec(url);
            if (!results) return null;
            if (!results[2]) return '';

            return decodeURIComponent(results[2].replace(/\+/g, ' '));
        }
    </script>
{% endapply %}
